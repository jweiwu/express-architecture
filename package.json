{
  "name": "express-mlab",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=production npm-run-all clean build server",
    "server": "node ./dist/bin/www.js",
    "build": "babel ./src --out-dir dist",
    "clean": "rimraf dist",
    "dev": "NODE_ENV=production nodemon --exec babel-node ./src/bin/www.js"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.17.3",
    "morgan": "~1.9.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "nodemon": "^1.19.1"
  }
}
